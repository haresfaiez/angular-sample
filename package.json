{
  "name": "Fundraising",
  "version": "0.0.1",
  "description": "Fundraising information system",
  "scripts": {
    "build": "tsc -p src/",
    "build:ts": "tsc -p src/",
    "build:sass": "node-sass src/ --include-path './bower_components/bootstrap-sass/assets/stylesheets/' --output-style compressed --output src/",
    "build:pug": "pug src/app src/index.pug",
    "start": "lite-server -c=test/manual/lite-server.conf.json",
    "test:factory:build": "tsc -p test/factory/",
    "test:micro:build": "tsc -p test/micro/src/",
    "test:micro:once": "karma start test/micro/karma.conf.js --single-run",
    "test:integration:build": "tsc -p test/integration/src/",
    "test:integration:once": "karma start test/integration/karma.conf.js --single-run"
  },
  "keywords": [],
  "author": "Elhares Faiez",
  "license": "",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "bower": "^1.8.0",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "lite-server": "^2.2.2",
    "node-sass": "^4.5.2",
    "pug": "^2.0.0-beta11",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.4",
    "typescript": "~2.1.0"
  },
  "repository": {}
}
